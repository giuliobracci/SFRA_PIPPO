<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
  <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="url">
                <display-name xml:lang="x-default">Emailblink Url</display-name>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>true</externally-managed-flag>
                <default-value>https://test.emailblink.com/api/subscribe</default-value>
            </attribute-definition>
                <attribute-definition attribute-id="client_id">
                <display-name xml:lang="x-default">Emailblink ID</display-name>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>pippo</default-value>
            </attribute-definition>
                <attribute-definition attribute-id="client_secret">
                <display-name xml:lang="x-default">Emailblink Secret</display-name>
                <type>string</type>
                <mandatory-flag>true</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>customer email</default-value>
            </attribute-definition>
            <group-definitions>
                <attribute-group group-id="Emailblink">
                    <display-name xml:lang="x-default">Emailblink Service Config</display-name>
                        <attribute attribute-id="url"/>
                        <attribute attribute-id="client_id"/>
                        <attribute attribute-id="client_secret"/>
                </attribute-group>
        </group-definitions>
        </custom-attribute-definitions>
    </type-extension>
</metadata>